@using Androtomist
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Androtomist</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Fonts -->
    <link rel="stylesheet" href="/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="/assets/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="/plugins/summernote/summernote-bs4.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="/assets/vendors/custom/jquery-ui/jquery-ui.bundle.css" rel="stylesheet" type="text/css" />
    <link href="/css/site.css?v=1.4" rel="stylesheet" type="text/css" />
    @RenderSection("ScriptsHeader", required: false)
</head>
<body>
    <div id="connectionLost" class="alert alert-warning fade show" role="alert" style="display:none; position:fixed; width: 100%; z-index: 9999;">
    </div>
    <div class="wrapper">
        <!-- begin:: Aside -->
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a class="nav-link settings" href="/account/account"><i class="fas fa-id-card"></i></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/account/logout"><i class="fas fa-exit"></i></a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/">
                <img alt="Logo" src="/assets/media/logo.png" width="200" style="margin-left: 20px;" />
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">

                        <li class="nav-header">Admin menu</li>
                        <li class="nav-item" aria-haspopup="true">
                            <a href="/dashboard/users/" class="nav-link @{ @(ViewContext.RouteData.Values["controller"].ToString() == "users" ? " active" : ""); } admin-option">
                                <i class="fa fa-users"></i>
                                <p>
                                    Users
                                </p>
                            </a>
                        </li>
                        <li class="nav-item" aria-haspopup="true">
                            <a href="/train/train/" class="nav-link @{ @(ViewContext.RouteData.Values["controller"].ToString() == "train" ? " active" : ""); } admin-option">
                                <i class="fa fa-vials"></i>
                                <p>
                                    Trainer
                                </p>
                            </a>
                        </li>
                        <li class="nav-item" aria-haspopup="true">
                            <a href="/classification/classification/" class="nav-link @{ @(ViewContext.RouteData.Values["controller"].ToString() == "classification" ? " active" : ""); } admin-option">
                                <i class="fa fa-brain"></i>
                                <p>
                                    Classification
                                </p>
                            </a>
                        </li>

                        <li class="nav-header">User menu</li>
                        <li class="nav-item">
                            <a href="/files/files/" class="nav-link  @{ @(ViewContext.RouteData.Values["controller"].ToString() == "files" ? " active" : ""); }">
                                <i class="fa fa-database"></i>
                                <p>
                                    Files
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/analysis/analysis/" class="nav-link  @{ @(ViewContext.RouteData.Values["controller"].ToString() == "analysis" ? " active" : ""); }">
                                <i class="fa fa-microscope"></i>
                                <p>
                                    Analyze
                                </p>
                            </a>
                        </li>
                        <!-- <li class="nav-item">
                            <a href="/results/results/" class="nav-link  @{ @(ViewContext.RouteData.Values["controller"].ToString() == "results" ? " active" : ""); }">
                                <i class="fa fa-chart-line"></i>
                                <p>
                                    Results
                                </p>
                            </a>
                        </li>-->
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">

                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <section class="content">
                @RenderBody()
            </section>
        </div>

        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
    </div>
    <!-- begin::Global Config(global config for global JS sciprts) -->
    <!-- jQuery -->
    <script src="/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="/plugins/sparklines/sparkline.js"></script>

    <!-- jQuery K/nob Chart -->
    <script src="/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="/plugins/moment/moment.min.js"></script>
    <script src="/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/dist/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->

    <script src="/js/site.js?v=1.5" type="text/javascript"></script>
    <script src="~/js/sweetalert2.all.min.js"></script>

    <script src="/js/dropzone.js" type="text/javascript"></script>

    <script type="text/javascript">
        $.ajaxSetup({
            timeout: 86400000
        });

        $(document).ready(function () {
            var evt = new CustomEvent('ScriptsLoaded', { param: 1 });
            window.dispatchEvent(evt);
        });

        $(".admin-option").click(function (e) {
            e.preventDefault();
            show_warning("This action is not permitted!", "This action is not permitted in demo accounts!");
        });

        $(".settings").click(function (e) {
            e.preventDefault();
            show_warning("This action is not permitted!", "This action is not permitted in demo accounts!");
        });

        $(document).on("click", ".editDisabled", function () {
            e.preventDefault();
            show_warning("This action is not permitted!", "This action is not permitted in demo accounts!");
        });

    </script>

    @RenderSection("ScriptsFooter", required: false)
</body>
</html>
